<?xml version="1.0" encoding="utf-8"?>

<!--suppress ALL -->
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.tripware"
          android:installLocation="preferExternal"
          android:versionCode="1"
          android:versionName="1.0">

    <application android:label="@string/app_name" android:icon="@drawable/tripwire">


        <activity android:name=".MainActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".ResultActivity"
                android:label="@string/result_activity" >
        </activity>
        <activity
                android:name=".SummaryActivity"
                android:label="@string/summary_activity" >
        </activity>
        <activity
                android:name=".FileOperations"
                android:label="@string/file_activity" >
        </activity>

        <activity
                android:name=".DifferntActivity"
                android:label="@string/diff_activity" >
        </activity>
    </application>



    <dependency org="com.googlecode.java-diff-utils" name="diffutils" rev="1.2.1"/>

    <uses-sdk android:minSdkVersion="19"> </uses-sdk>



    <uses-permission android:name="android.permission.INTERNET"/>

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />



    <uses-permission android:name="android.permission.ACCESS_SUPERUSER" />


    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />










</manifest>